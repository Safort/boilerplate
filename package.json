{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "just another boilerplate",
  "main": "index.js",
  "author": "Safort",
  "license": "ISC",
  "scripts": {
    "start": "node ./app/index",
    "build": "npm run build-backend && npm run build-frontend",
    "prod:front": "webpack-cli --mode=production",
    "dev:front": "webpack serve",
    "build:backend": "tsc ./src/backend/index --outDir ./app",
    "watch:backend": "tsc ./src/backend/index --outDir ./app --watch",
    "lint": "eslint ./src --ext=jsx,tsx,js,ts",
    "typecheck": "tsc -p tsconfig.json",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "test": ""
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/koa": "^2.13.3",
    "@types/koa-router": "^7.4.2",
    "@types/koa-static": "^4.0.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.24.0",
    "eslint-webpack-plugin": "^2.5.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "cookie-parser": "^1.4.5",
    "effector": "^21.8.12",
    "effector-react": "^21.3.2",
    "history": "^5.0.0",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
