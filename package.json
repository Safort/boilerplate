{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "just another boilerplate",
  "main": "index.js",
  "author": "Safort",
  "license": "ISC",
  "scripts": {
    "start": "node ./app/index",
    "build": "npm run build-backend && npm run build-frontend",
    "prod:front": "webpack-cli --mode=production",
    "dev:front": "webpack serve",
    "build:backend": "tsc ./src/backend/index --outDir ./app",
    "watch:backend": "tsc ./src/backend/index --outDir ./app --watch",
    "lint": "eslint ./src",
    "typecheck": "tsc -p tsconfig.json",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.32",
    "@types/koa-static": "^4.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-env": "^1.13.6",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "husky": "^4.3.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "cookie-parser": "^1.4.5",
    "history": "^5.0.0",
    "koa": "^2.13.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  }
}
