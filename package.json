{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "just another boilerplate",
  "main": "index.js",
  "author": "Safort",
  "license": "ISC",
  "scripts": {
    "start": "node ./app/index",
    "build": "npm run build-backend && npm run build-frontend",
    "build-frontend": "tcm src -o src_dts && webpack-cli --mode=production",
    "watch-frontend": "tcm src -o src_dts && webpack-cli --mode=development",
    "build-backend": "tsc ./src/backend/index --outDir ./app",
    "watch-backend": "tsc ./src/backend/index --outDir ./app --watch",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "@types/koa": "^2.0.44",
    "@types/koa-router": "^7.0.27",
    "css-loader": "^0.28.11",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^3.1.0",
    "postcss-loader": "^2.1.3",
    "precss": "^2.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.1.0",
    "typed-css-modules": "^0.3.1",
    "typescript": "^2.8.1",
    "webpack": "^4.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "cookie-parser": "^1.4.1",
    "history": "^4.7.2",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
