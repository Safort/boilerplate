{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "just another boilerplate",
  "main": "index.js",
  "author": "Safort",
  "license": "ISC",
  "scripts": {
    "start": "node ./app/index",
    "build": "npm run build-backend && npm run build-frontend",
    "build-frontend": "tcm src -o src_dts && webpack-cli --mode=production",
    "watch-frontend": "tcm src -o src_dts && webpack-dev-server --mode development -w --progress",
    "build-backend": "tsc ./src/backend/index --outDir ./app",
    "watch-backend": "tsc ./src/backend/index --outDir ./app --watch",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.31",
    "@types/webpack-env": "^1.13.6",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.1.0",
    "typed-css-modules": "^0.3.5",
    "typescript": "^3.0.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "cookie-parser": "^1.4.1",
    "history": "^4.7.2",
    "koa": "^2.5.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
